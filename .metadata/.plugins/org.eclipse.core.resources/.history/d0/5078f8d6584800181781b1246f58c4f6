package t2;

public class Main {
	class Thread1 implements Runnable {
		int result;
		boolean flag = true;

		public int getResult() {
			return this.result;
		}

		@Override
		public void run() {
			while (flag) {
				result++;
				try {
					Thread.sleep(300);
				} catch (InterruptedException e) {
					e.printStackTrace();
				}
			}
		}
	}

	public static void main(String[] args) {

	}

}
